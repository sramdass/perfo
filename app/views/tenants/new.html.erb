<%= render 'shared/admin_nav_links', :active => "tenants" %>
<div class="span6">
  <%= simple_form_for @tenant, :html => { :class => 'form-horizontal' } do |f| %>
  <legend> New Tenant </legend>
    <%= render 'tenant_fields', :f => f %>
    
  <div class="form-actions">
    <%= button_tag(:type => 'submit', :name => 'create', :class => "btn btn-success btn-large" ) do %>
      <i class="icon-ok icon-white"> </i> <strong> Create </strong>
    <% end %>
    <%= link_to tenants_path, :class => 'btn btn-danger btn-large' do  %>
       <i class="icon-off icon-white"> </i> <strong> Cancel </strong>
    <% end %>
  </div>  
  <% end %>
    <%= render 'shared/controller_action_buttons', :obj => @tenant, :action => "new" %>      
</div>
<div class="span3">
  <%= render 'shared/semi_index', :model_name => "Tenant",
																:columns => ['name', 'activated'] %>
</div>



